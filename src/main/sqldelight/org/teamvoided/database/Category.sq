CREATE TABLE IF NOT EXISTS category(
    id SMALLSERIAL PRIMARY KEY,
    name text NOT NULL UNIQUE,
    type text NOT NULL
);

selectById:
SELECT *
FROM category
WHERE id = :id;


selectByType:
SELECT *
FROM category
WHERE type = :type;


selecAll:
SELECT *
FROM category;

create:
INSERT INTO category( name, type)
VALUES (:name, :type)
RETURNING id;
